{
    "csharp.format.enable": false,
    "git.autofetch": true,
    "window.zoomLevel": -1.0,
    "explorer.confirmDelete": false,
    "html.format.wrapLineLength": 0,
    "files.autoSave": "onWindowChange",
    "editor.fontSize": 12,
    "editor.cursorStyle": "block",
    "editor.cursorBlinking": "solid",
    "workbench.editor.tabCloseButton": "left",
    "workbench.startupEditor": "newUntitledFile",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootPath}",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.idea": true,
        "**/_ReSharper.Caches": true,
        "**/node_modules": true,
        "**/out": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.sortOrder": "type",
    "markdown.preview.fontSize": 13,
    "extensions.ignoreRecommendations": true,
    "telemetry.enableTelemetry": false,
    "git.confirmSync": false,
    "workbench.colorTheme": "Better Solarized Dark",
    "workbench.iconTheme": "vscode-great-icons",
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'DejaVu Sans Mono', 'monospace', monospace",
    "search.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.idea": true,
        "**/_ReSharper.Caches": true,
        "**/node_modules": true,
        "**/out": true
    },
    "terminal.external.linuxExec": "sakura",
    "css.fileExtensions": [
        "css",
        "scss",
        "sass",
        "less"
    ],
    "dismpd.ip": "localhost",
    "dismpd.port": 1337
}